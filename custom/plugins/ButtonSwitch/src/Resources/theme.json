{
  "name": "ButtonSwitch",
  "author": "Shopware AG",
  "views": [
     "@Storefront",
     "@Plugins",
     "@ButtonSwitch"
  ],
  "style": [
    "app/storefront/src/scss/overrides.scss",
    "@Storefront",
    "app/storefront/src/scss/base.scss"
  ],
  "script": [
    "@Storefront",
    "app/storefront/dist/storefront/js/button-switch.js"
  ],
  "asset": [
    "@Storefront",
    "app/storefront/src/assets"
  ],
  "config": {
    "blocks": {
      "BuyFeedback": {
        "label": {
          "en-GB": "Buy Feedback"
          }
        }
      },
      
      "fields": {
        "activateBuyButtenFeedback": {
          "label": {
            "en-GB": "Use Buy Button Feedback"        
          },
          "type": "switch",
          "custom": {
            "bordered": true
          },
          "value": false,
          "editable": true,
          "block": "BuyFeedback"
        },
      
        "sw-colorpicker_input": {
          "label": {
            "en-GB": "Buy Button Feedback Color"
          },
          "type": "int",
          "editable": true,
          "block": "BuyFeedback" 
        }
        
        
        
        
    }
  }
}
